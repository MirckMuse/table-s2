{
	"name": "table-s2",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"files": [
		"dist",
		"index.d.ts"
	],
	"main": "./dist/S2Table.umd.cjs",
	"module": "./dist/S2Table.js",
	"types": "./index.d.ts",
	"exports": {
		"types": "./index.d.ts",
		"import": "./dist/S2Table.js",
		"require": "./dist/S2Table.umd.cjs"
	},
	"scripts": {
		"dev": "vite --host",
		"build": "tsc && vite build",
		"prepare": "husky install"
	},
	"devDependencies": {
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"rome": "^12.1.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "rome check"
		}
	},
	"lint-staged": {
		"lib/**/*.{js,ts}": [
			"echo 'from lint-stage'"
		]
	},
	"dependencies": {
		"@antv/s2": "2.0.0-next.7"
	}
}
